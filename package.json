    "plugin-error": "^1.0.1",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.2.0",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.0",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "style-loader": "^1.3.0",
    "swiper": "^6.5.0",
    "tailwindcss": "^2.0.3",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^8.0.17",
